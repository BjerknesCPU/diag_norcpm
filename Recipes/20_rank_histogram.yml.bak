# Rank histogram
Title: SST regional Rank histogram, 1-4 lead season
Description: |
    Skipped
COMPONENT:  ocn
VARIABLE:  sst
PLOTRES: |
    ;; PLOTRES
OBSPROXY: 0
    # 0: observation; >0: which member as obs proxy data
REMOVECACHE: False

Scripts:
# proc section
-  # lead season 1, init Jan, global
    plotScript: plot_rank_histogram.ncl
    TITLE: SST REG rank histogram
    TITLELEFT: REG
    FIGFILENAME: sst_REG_rank_histogram_lead1_FMA
    LEADSEASON: 1
    REG: global
    LATB: -90.
    LATE:  90.
    LONB:   0.
    LONE: 360.
    CACHEFILENAME: sst_rank_freq_lead1_FMA  # cache is made global
    # for all forecast start from Jan
    FORECASTDIRS: BASEDIR/RUNPRE{1,2}???01??
-  # lead season 1, init Jan, NAtl
    plotScript: plot_rank_histogram.ncl
    TITLE: SST REG rank histogram
    TITLELEFT: REG
    FIGFILENAME: sst_REG_rank_histogram_lead1_FMA
    LEADSEASON: 1
    REG: NAtl
    LATB:   20.
    LATE:   70.
    LONB: -110.
    LONE:   40.
    CACHEFILENAME: sst_rank_freq_lead1_FMA  # cache is made global
    # for all forecast start from Jan
    FORECASTDIRS: BASEDIR/RUNPRE{1,2}???01??
-  # lead season 1, init Jan, WNPac
