    REG: WNPac
    LATB:   20.
    LATE:   70.
    LONB:  110.
    LONE:  180.
    CACHEFILENAME: sst_rank_freq_lead1_FMA  # cache is made global
    # for all forecast start from Jan
    FORECASTDIRS: BASEDIR/RUNPRE{1,2}???01??

-  # lead season 1, init Apr, global
